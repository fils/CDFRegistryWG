    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Subject Area Scheme</rdfs:label>
    <rdfs:comment xml:lang="en">The specific scheme for a subject area.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/subjectScheme</skos:relatedTo>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://example.org/voc/SubjectScheme">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Subject Scheme</rdfs:label>
    <rdfs:comment xml:lang="en">The specific scheme for a subject area.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/subjectScheme</skos:relatedTo>
  </owl:Class>

