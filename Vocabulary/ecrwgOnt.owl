    <rdfs:label xml:lang="en">Subject Area</rdfs:label>
    <rdfs:comment xml:lang="en">A disciplinary focus.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/subject</skos:relatedTo>
  </owl:Class>

