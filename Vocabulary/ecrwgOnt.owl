<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <owl:Ontology rdf:about="http://example.org/voc/">
    <dc11:description>Schema.org extension for a Data Repository</dc11:description>
    <dc11:title>EarthCube Registry Working Group Vocabulary</dc11:title>
    <dc11:identifier>EC RWG Vocabulary v0.1</dc11:identifier>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2011-05-11</dc:issued>
    <dc11:creator rdf:resource="https://raw.githubusercontent.com/fils/CDFRegistryWG/master/Vocabulary/some-old-joe-schmoe"/>
  </owl:Ontology>

  <owl:Class rdf:about="http://example.org/voc/DataRepository">
    <rdfs:subClassOf rdf:resource="http://schema.org/Organization"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Data Repository</rdfs:label>
    <rdfs:comment xml:lang="en">A Data Repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repository</skos:relatedTo>
  </owl:Class>

  <owl:ObjectProperty rdf:about="http://example.org/voc/identifiers">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/identifier"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://examples.org/voc/Identifier"/>
    <rdfs:label xml:lang="en">Identifier</rdfs:label>
    <rdfs:comment xml:lang="en">Links to identifiers that uniquely identify the data repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/identifiers</skos:relatedTo>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://example.org/voc/Identifier">
    <rdfs:subClassOf rdf:resource="http://schema.org/PropertyValue"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Data Repository Identifier</rdfs:label>
    <rdfs:comment xml:lang="en">A unique identifier with a type and value for the Data Repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryIdentifier</skos:relatedTo>
  </owl:Class>

  <owl:ObjectProperty rdf:about="http://example.org/voc/identifierType">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/propertyID"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/Identifier"/>
    <rdfs:range rdf:resource="http://example.org/voc/IdentifierType"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">An identifier's type such as DOI, ARK, URN or for custom types like Re3Data. For example a DOI of doi:10.1234/567890 would have the identifier value of '&lt;http://example.org/voc/IdentifierTypeDOI&gt;'</rdfs:label>
    <rdfs:comment xml:lang="en">The type that identifies the domain space for its uniqueness.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryIdentifierType</skos:relatedTo>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://example.org/voc/IdentifierType">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Data Repository Identifier Type</rdfs:label>
    <rdfs:comment xml:lang="en">A identifier type for an Identifier.</rdfs:comment>
  </owl:Class>

  <owl:namedIndividual rdf:about="http://example.org/voc/IdentifierTypeDOI">
    <rdf:type rdf:resource="http://example.org/voc/IdentifierType"/>
    <rdfs:label xml:lang="en">Digital Object Identifier (DOI)</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/IdentifierTypeRe3data">
    <rdf:type rdf:resource="http://example.org/voc/IdentifierType"/>
    <rdfs:label xml:lang="en">Specifically a Re3Data Digital Object Identifier (DOI)</rdfs:label>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/re3data</skos:relatedTo>
  </owl:namedIndividual>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/identifierValue">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/value"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/Identifier"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">An identifier's value.</rdfs:label>
    <rdfs:comment xml:lang="en">The value that will be unique within a specifc identifier type. For example a DOI of doi:10.1234/567890 would have the identifier value of '10.1234/567890'</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryIdentifierValue</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/identifierUrl">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/url"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/Identifier"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label xml:lang="en">An identifier's resolvable URL.</rdfs:label>
    <rdfs:comment xml:lang="en">The url that will resolve to the identified thing. For example a DOI of doi:10.1234/567890 could have the identifier url of 'https://doi.org/10.1234/567890' and/or 'http://doi.org/10.1234/567890' and/or 'http://dx.doi.org/10.1234/567890', hence it's unbounded cardinality.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryIdentifierValue</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/repositoryName">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/name"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Repository Name</rdfs:label>
    <rdfs:comment xml:lang="en">The name of the data repository expressed in text.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryName</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/additionalName">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/additionalName"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Additional Name</rdfs:label>
    <rdfs:comment xml:lang="en">The alternative name or acronym for the data repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/alternateName</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/repositoryUrl">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/url"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Data Repository URL</rdfs:label>
    <rdfs:comment xml:lang="en">The canonical URL of the data repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryUrl</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/description">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/description"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Data Repository Description</rdfs:label>
    <rdfs:comment xml:lang="en">A textual description providing additional information about the Data Repository (primary language is English). The language should be defined by the xsd datatype of the value. For example, '@en'. See </rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/description</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://example.org/voc/contactPoint">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/contactPoint"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://example.org/voc/ContactPoint"/>
    <rdfs:label xml:lang="en">Data Repository Contact</rdfs:label>
    <rdfs:comment xml:lang="en">The agent to contact.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryContact</skos:relatedTo>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://example.org/voc/ContactPoint">
    <rdfs:subClassOf rdf:resource="http://schema.org/ContactPoint"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Contact Point</rdfs:label>
    <rdfs:comment xml:lang="en">The contact point with either an email address or URL for an online contact form.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryContact</skos:relatedTo>
  </owl:Class>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/contactEmail">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/email"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/ContactPoint"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Contact Point Email</rdfs:label>
    <rdfs:comment xml:lang="en">An email address for a data repository's contact point.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryContact</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/contactUrl">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/url"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/ContactPoint"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Contact Point URL</rdfs:label>
    <rdfs:comment xml:lang="en">URL for an online contact form.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryContact</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://example.org/voc/repositoryType">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/additionalType"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://example.org/voc/RepositoryType"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <rdfs:label xml:lang="en">Data Repository Type</rdfs:label>
    <rdfs:comment xml:lang="en">The type of repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryTypes</skos:relatedTo>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://example.org/voc/RepositoryType">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:label xml:lang="en">Repostory Type</rdfs:label>
    <rdfs:comment xml:lang="en">The type of repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/type</skos:relatedTo>
  </owl:Class>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeDisciplinary">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Disciplinary repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeGovernmental">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Governmental repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeInstitutional">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Institutional repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeMultidisciplinary">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Multidisciplinary repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeProjectRelated">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Project-related repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:namedIndividual rdf:about="http://example.org/voc/RepositoryTypeOther">
    <rdf:type rdf:resource="http://example.org/voc/RepositoryType"/>
    <rdfs:label xml:lang="en">Other repository type</rdfs:label>
  </owl:namedIndividual>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/size">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/numberOfItems"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Size of Data Repository</rdfs:label>
    <rdfs:comment xml:lang="en">The number of items contained in the repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/size</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/startDate">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/foundingDate"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Releasing Date</rdfs:label>
    <rdfs:comment xml:lang="en">Releasing date of the repository.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/startDate</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/endDate">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Releasing Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date the repository ended its service of ingesting new research data and/or providing it.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/endDate</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/closedDate">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Closed Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date when the repository stopped ingesting new research data to its database. The database and its research data are still available.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/closed</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/offlineDate">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/dissolutionDate"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdfs:label xml:lang="en">Closed Date</rdfs:label>
    <rdfs:comment xml:lang="en">The date when the repository went offline. The database and/or the website are no longer available.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/offline</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/userInterfaceLanguage">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/inLanguage"/>
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/DataRepository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#language"/>
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
    <rdfs:label xml:lang="en">Repository Language</rdfs:label>
    <rdfs:comment xml:lang="en">The user interface language(s) of the RDR.</rdfs:comment>
    <skos:relatedTo>http://www.re3data.org/schema/3-0/repositoryLanguage</skos:relatedTo>
  </owl:DatatypeProperty>

