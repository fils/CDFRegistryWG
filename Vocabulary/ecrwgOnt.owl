    <skos:relatedTo>http://www.re3data.org/schema/3-0/subjectId</skos:relatedTo>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/voc/subjectName">
    <rdfs:isDefinedBy rdf:resource="http://example.org/voc/"/>
    <rdfs:domain rdf:resource="http://example.org/voc/SubjectArea"/>
