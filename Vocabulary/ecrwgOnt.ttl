<http://example.org/voc/>
    a owl:Ontology ;
    dc11:description "Schema.org extension for a Data Repository" ;
    dc11:title "EarthCube Registry Working Group Vocabulary" ;
    dc11:identifier "EC RWG Vocabulary v0.1" ;
    dc:issued "2011-05-11"^^xsd:date ;
    dc11:creator <some-old-joe-schmoe> .

<http://example.org/voc/DataRepository>
    a owl:Class ;
    rdfs:subClassOf <http://schema.org/Organization> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:label "Data Repository"@en ;
    rdfs:comment "A Data Repository."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repository" .

<http://example.org/voc/identifiers>
    a owl:ObjectProperty ;
    rdfs:subPropertyOf <http://schema.org/identifier> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/DataRepository> ;
    rdfs:range <http://examples.org/voc/Identifier>;
    rdfs:label "Identifier"@en ;
    rdfs:comment "Links to identifiers that uniquely identify the data repository."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/identifiers" .

<http://example.org/voc/Identifier>
    a owl:Class ;
    rdfs:subClassOf <http://schema.org/PropertyValue> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:label "Data Repository Identifier"@en ;
    rdfs:comment "A unique identifier with a type and value for the Data Repository."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryIdentifier" .

# see: Example 1 JSON-LD at: http://schema.org/identifier
<http://example.org/voc/identifierType>
    a owl:ObjectProperty ;
    rdfs:subPropertyOf <http://schema.org/propertyID> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/Identifier> ;
    rdfs:range <http://example.org/voc/IdentifierType> ;
    owl:minCardinality 1^^xsd;nonNegativeInteger ;
    owl:maxCardinality 1^^xsd;nonNegativeInteger ;
    rdfs:label "An identifier's type such as DOI, ARK, URN or for custom types like Re3Data. For example a DOI of doi:10.1234/567890 would have the identifier value of '<http://example.org/voc/IdentifierTypeDOI>'"@en ;
    rdfs:comment "The type that identifies the domain space for its uniqueness."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryIdentifierType" .

<http://example.org/voc/IdentifierType>
    a owl:Class ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:label "Data Repository Identifier Type"@en ;
    rdfs:comment "A identifier type for an Identifier."@en .

<http://example.org/voc/IdentifierTypeDOI>
    a owl:namedIndividual ;
    rdf:type <http://example.org/voc/IdentifierType> ;
    rdfs:label "Digital Object Identifier (DOI)"@en .

<http://example.org/voc/IdentifierTypeRe3data>
    a owl:namedIndividual ;
    rdf:type <http://example.org/voc/IdentifierType> ;
    rdfs:label "Specifically a Re3Data Digital Object Identifier (DOI)"@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/re3data" .

<http://example.org/voc/identifierValue>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/value> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/Identifier> ;
    rdfs:range xsd:string ;
    owl:minCardinality 1^^xsd;nonNegativeInteger ;
    owl:maxCardinality 1^^xsd;nonNegativeInteger ;
    rdfs:label "An identifier's value."@en ;
    rdfs:comment "The value that will be unique within a specifc identifier type. For example a DOI of doi:10.1234/567890 would have the identifier value of '10.1234/567890'"@en ;
    # NOTE: re3data has this bound to a URI so this is not the same.
    # Should we talk through why not using the URI is better for Linked Data federated queries.
    # @see added property here to include re3data identifier url: <http://example.org/voc/identifierUrl>
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryIdentifierValue" .

<http://example.org/voc/identifierUrl>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/url> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/Identifier> ;
    rdfs:range xsd:anyURI ;
    rdfs:label "An identifier's resolvable URL."@en ;
    rdfs:comment "The url that will resolve to the identified thing. For example a DOI of doi:10.1234/567890 could have the identifier url of 'https://doi.org/10.1234/567890' and/or 'http://doi.org/10.1234/567890' and/or 'http://dx.doi.org/10.1234/567890', hence it's unbounded cardinality."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryIdentifierValue" .

