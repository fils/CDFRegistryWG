@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix cert: <http://www.w3.org/ns/auth/cert#> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix exif: <http://www.w3.org/2003/12/exif/ns#> .
@prefix fn: <http://www.w3.org/2005/xpath-functions#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geonames: <http://www.geonames.org/ontology#> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rei: <http://www.w3.org/2004/06/rei#> .
@prefix rsa: <http://www.w3.org/ns/auth/rsa#> .
@prefix rss: <http://purl.org/rss/1.0/> .
@prefix sfn: <http://www.w3.org/ns/sparql#> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swrc: <http://swrc.ontoware.org/ontology#> .
@prefix types: <http://rdfs.org/sioc/types#> .
@prefix wot: <http://xmlns.com/wot/0.1/> .
@prefix xhtml: <http://www.w3.org/1999/xhtml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ecrwg: <http://example.org/voc/> .

<http://example.org/voc/>
    a owl:Ontology ;
    dc11:description "Schema.org extension for a Data Repository" ;
    dc11:title "EarthCube Registry Working Group Vocabulary" ;
    dc11:identifier "EC RWG Vocabulary v0.1" ;
    dc:issued "2011-05-11"^^xsd:date ;
    dc11:creator <some-old-joe-schmoe> .

<http://example.org/voc/DataRepository>
    a owl:Class ;
    rdfs:subClassOf <http://schema.org/Organization> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:label "Data Repository"@en ;
    rdfs:comment "A Data Repository."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repository" .

<http://example.org/voc/contactPoint>
    a owl:ObjectProperty ;
    rdfs:subPropertyOf <http://schema.org/contactPoint> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/DataRepository> ;
    rdfs:range <http://example.org/voc/Organization> ;
    rdfs:label "Has Business Contact"@en ;
    rdfs:comment "Links to information about a business location for an organization."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryContact" ;
    
    
    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .

<http://example.org/voc/repositoryName>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/name> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain  <http://example.org/voc/DataRepository> ;
    rdfs:range xsd:string ;
    rdfs:label "Repository Name"@en ;
    rdfs:comment "The name of the data repository expressed in text."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/repositoryName" .

<http://example.org/voc/remarks>
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/DataRepository> ;
    rdfs:range xsd:string ;
    rdfs:label "Informational remarks"@en ;
    rdfs:comment "Free text remarks about the respository."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/XXXX" .

<http://example.org/voc/lastUpdate>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/XXX> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/DataRepository> ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Date of last update"@en ;
    rdfs:comment "The date from which last update was made"@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/XXXX" .

<http://example.org/voc/entryDate>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/XXX> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/DataRepository> ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Date of entry"@en ;
    rdfs:comment "The date entry made"@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/XXXX" .

<http://example.org/voc/AlertService>
    a owl:Class ;
    rdfs:subClassOf <http://schema.org/ItemList> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:label "Data Repository"@en ;
    rdfs:comment "The alerting service the data repository offers "@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/syndication" .

<http://example.org/voc/alertServiceType>
    a owl:DatatypeProperty ;
    # no obvious subproperty in http://schema.org/ItemList
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/AlertService> ;
    rdfs:range xsd:string ;
    owl:minCardinality 1^^xsd;nonNegativeInteger ;
    owl:maxCardinality 1^^xsd;nonNegativeInteger ;
    rdfs:label "Alert Service Type"@en ;
    rdfs:comment "Alerting service type like RSS."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/syndicationType" .

<http://example.org/voc/alertServiceType>
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf <http://schema.org/url> ;
    rdfs:isDefinedBy <http://example.org/voc/> ;
    rdfs:domain <http://example.org/voc/AlertService> ;
    rdfs:range xsd:anyURI ;
    owl:minCardinality 1^^xsd;nonNegativeInteger ;
    owl:maxCardinality 1^^xsd;nonNegativeInteger ;
    rdfs:label "Alert Service URL"@en ;
    rdfs:comment "Alerting service URL like 'http://example.org/rss.xml'."@en ;
    skos:relatedTo "http://www.re3data.org/schema/3-0/syndicationUrl" .
